b == a
print (a is b) #T 

a = 5
print(a is 5)           # T 

b = 10
print(a is b)           # F 

c = "hello"
print(c is "hello")     # T 

d = "world"
print(c is d)           # F 

e = True
print(e is True)        # T 

f = False
print(e is f)           # False
